#include <stdio.h>

int main(){
    double massive [3][3];
    int i,j; 
    double main=0, notmain=0; 
    printf("Введите через пробел значения для матрицы третьего порядка: \n");
    for (i=0; i<3; i++) 
    {
        for(j=0;j<3;j++)
        {
            scanf("%lf", &massive[i][j]); 
        }
    }
    for (i=0; i<3; i++)
    {
        main += massive [i][i]; 
        notmain += massive [i][2-i]; 
    }
    printf("Сумма элементов на главной диагонали: %lf\n", main); 
    printf("Сумма элементов на побочной диагонали: %lf\n", notmain);
    int matrix_sec [2][2];
printf("Введите значения для матрицы второго порядка:\n");
 for (i = 0; i < 2; i++) {
    for (j = 0; j < 2; j++) {
        scanf("%d", &matrix_sec[i][j]);
    }
 }
 printf("Квадрат матрицы второго порядка:\n");
 for (i = 0; i < 2; i++) {
     for (j = 0; j < 2; j++) {
        qw= 
          printf("%d ", matrix_sec[i][j] * matrix_sec[i][j]);
     }
        printf("\n");
     }
    return 0;
}
